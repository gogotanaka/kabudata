<%= image_tag('inquiry_bar.jpg')%>

<%= form_for @inquiry, url: {action: 'inquiry_create'} do |f| %>
  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </div>
  <div class="field">
    <%= f.label :content %><br />
    <%= f.text_field :content %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
