<%= image_tag(@bar)%>
<table class="stock_table">
  <tr >
  	<% @ths.each.with_index(0) do |th,i| %>
  		<% if @impo[i]==0  %>
  			<th><%= th %></th>
  		<% else %>
  			<th colspan="<%= @impo[i].to_s  %>"><%= th %></th>
  		<% end %>
  	<% end %>
  </tr>
	<% @tds.each do |tds| %>
		<tr>
			<% tds.each do |td| %>
				<td><%= td %></td>
			<% end %>
		</tr>
	<% end %>
</table>

